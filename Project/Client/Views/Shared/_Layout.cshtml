    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css" integrity="sha512-sMXtMNL1zRzolHYKEujM2AqCLUR9F2C4/05cdbxjjLSRvMQIciEPCQZo++nk7go3BtSuK9kfa/s+a4f4i5pLkw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body style="background-color: rgba(255, 255, 255, 0.9);">

    <div id="google_element">

    </div>

    <div class="container-fluid">
        <header>
            <nav class="navbar navbar-expand-lg navbar-light">
                <div class="container-fluid">
                    <div class="d-flex w-100">
                        <li class="col-md-2 logo me-auto" style="list-style-type: none;padding-left: 0;">
                            <a asp-controller="Home" asp-action="Index">
                                <img src="~/Image/logo.png">
                            </a>
                        </li>
                        <button class="navbar-toggler ms-auto" style="padding: 0.25rem 0.5rem; outline:none;" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon" style="width: 20px;height: 20px;"></span>
                        </button>
                    </div>
                    <form class="col-md-5 d-flex my-2 my-lg-0 justify-content-center">
                        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                        <button class="btn btn-outline-success" type="submit" >Search</button>
                    </form>
                    <div class="col-md-5 collapse navbar-collapse" id="navbarSupportedContent" style="outline:none;border:none;">
                        <ul class="navbar-nav d-flex justify-content-around w-100 mb-2 mb-lg-0">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle btn fw-bold" style="border: .3px solid #00A97F" href="#" id="navbarDropdown1" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Platforms
                                </a>
                                <ul class="dropdown-menu" aria-labelledby="navbarDropdown1">
                                    <li><a class="navelement" href="#">Windows</a></li>
                                    <li><a class="navelement" href="#">MacOS</a></li>
                                    <li><a class="navelement" href="#">Android</a></li>
                                </ul>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle btn fw-bold" style="border: .3px solid #00A97F" href="#" id="navbarDropdown2" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Categories
                                </a>
                                <ul class="dropdown-menu" aria-labelledby="navbarDropdown2">
                                    <li><a class="navelement" href="#">Action</a></li>
                                    <li><a class="navelement" href="#">Another action</a></li>
                                    <li><a class="navelement" href="#">Something else here</a></li>
                                </ul>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle btn fw-bold" style="border: .3px solid #00A97F" href="#" id="navbarDropdown3" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Profile
                                </a>
                                <ul class="dropdown-menu" aria-labelledby="navbarDropdown3">
                                    <li><a class="navelement" asp-controller="User" asp-action="Register">Register</a></li>
                                    <li><a class="navelement" asp-controller="User" asp-action="Login">Login</a></li>
                                    <li><a class="navelement" asp-controller="Order" asp-action="History">History</a></li>
                                    <li><a class="navelement" asp-controller="User" asp-action="Information">Information</a></li>
                                    <li><a class="navelement" asp-controller="User" asp-action="ChangePassword">ChangePassword</a></li>
                                    <li><a class="navelement" asp-controller="Cart" asp-action="Cart">Cart</a></li>
                                    <li><a class="navelement" asp-controller="Product" asp-action="ProductDetail">ProductDetail</a></li>
                                    <li><a class="navelement" asp-controller="product" asp-action="Collection">Collection</a></li>
                                    <li><a class="navelement" asp-controller="Admin" asp-action="AdminDashboard">AdminDashboard</a></li>
                                    <li><a class="navelement" asp-controller="User" asp-action="EditProfile">EditProfile</a></li>
                                    
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>


        </header>



        <div class="container mb-5" style="min-height:1000px;">
            <main role="main">
                @RenderBody()
            </main>
        </div>
        <footer class="footer">
            <div class="container_footer">
                <div class="row">
                    <div class="col-12 col-sm-6 col-md-3 footer_col">
                        <h4>Company</h4>
                        <ul>
                            <li><a asp-controller="" asp-action="">Abour Us</a></li>
                            <li><a asp-controller="" asp-action="">Our Service</a></li>
                            <li><a asp-controller="" asp-action="">Pravacy Policy</a></li>
                            <li><a asp-controller="" asp-action="">Affiliate Program</a></li>
                        </ul>
                    </div>
                    <div class="col-12 col-sm-6 col-md-3 footer_col">
                        <h4>Get Help</h4>
                        <ul>
                            <li><a asp-controller="" asp-action="">FAQ</a></li>
                            <li><a asp-controller="" asp-action="">Shipping</a></li>
                            <li><a asp-controller="" asp-action="">Returns</a></li>
                            <li><a asp-controller="" asp-action="">Order Status</a></li>
                            <li><a asp-controller="" asp-action="">Payment Options</a></li>
                        </ul>
                    </div>
                    <div class="col-12 col-sm-6 col-md-3 footer_col">
                        <h4>Online Shop</h4>
                        <ul>
                            <li><a asp-controller="" asp-action="">Watch</a></li>
                            <li><a asp-controller="" asp-action="">Bag</a></li>
                            <li><a asp-controller="" asp-action="">Shoes</a></li>
                            <li><a asp-controller="" asp-action="">Dress</a></li>
                        </ul>
                    </div>
                    <div class=" col-12 col-sm-6 col-md-3 footer_col">
                        <h4>Follow US</h4>
                        <div class="social-links">
                            <a asp-controller="" asp-action="">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                            <a asp-controller="" asp-action="">
                                <i class="fab fa-twitter"></i>
                            </a>
                            <a asp-controller="" asp-action="">
                                <i class="fab fa-instagram"></i>
                            </a>
                            <a asp-controller="" asp-action="">
                                <i class="fab fa-linkedin-in"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
